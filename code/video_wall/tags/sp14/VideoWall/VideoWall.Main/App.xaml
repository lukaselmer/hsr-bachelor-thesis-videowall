<Application x:Class="VideoWall.Main.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="ApplicationStartup"
             xmlns:d="http://schemas.microsoft.com/expression/interactivedesigner/2006"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d">
    <Application.Resources>
        <!--StartupUri="PosterWindow.xaml"-->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/VideoWall.ResourceLoader;component/Resources.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/VideoWall.ResourceLoader;component/Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
