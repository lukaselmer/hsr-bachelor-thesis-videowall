<UserControl x:Class="UserInterface.ArrowRight"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Canvas Background="White" Height="150" Width="75">
        <Polygon Points="0,0 0,150 75,75 0,0">
            <Polygon.Fill>
                <SolidColorBrush Color="Blue" Opacity="0.8"/>
            </Polygon.Fill>
        </Polygon>
    </Canvas>
</UserControl>